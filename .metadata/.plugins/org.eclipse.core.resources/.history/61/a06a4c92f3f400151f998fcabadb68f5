package lab06;

public class Person {
	private String name;
	private String gender;
	private String id;

	public String getId() {
		return id;
	}

	public String getName(){
		return name;
	}
	
	public String getGender(){
		return gender;
	}
	
	Person (String Id, String Name, String Gender){
		id=Id;
		name = Name;
		gender = Gender;
	}
}

